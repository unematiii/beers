{
    "compilerOptions": {
        "baseUrl": "./",
        "sourceMap": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "downlevelIteration": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ],
}